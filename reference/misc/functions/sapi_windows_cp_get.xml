<refentry xml:id="function.sapi_windows_cp_get" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>sapi_windows_cp_get</refname>
  <refpurpose>Get process codepage</refpurpose>
 </refnamediv>

 <refsect1 role="description"><!-- {{{ -->
  &reftitle.description;
  <methodsynopsis role="procedural">
   <type>int</type><methodname>sapi_windows_cp_get</methodname>
   <methodparam><type>string</type><parameter>kind</parameter></methodparam>
  </methodsynopsis>
  <para>
   Get the identifier of the codepage of the current process.
  </para>
 </refsect1><!-- }}} -->

 <refsect1 role="parameters"><!-- {{{ -->
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>kind</parameter></term>
    <listitem>
     <para>
      The kind of codepage: either <literal>'ansi'</literal> or <literal>'oem'</literal>.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1><!-- }}} -->

 <refsect1 role="returnvalues"><!-- {{{ -->
  &reftitle.returnvalues;
  <para>
   Returns the codepage identifier.
  </para>
 </refsect1><!-- }}} -->

 <refsect1 role="seealso"><!-- {{{ -->
  &reftitle.seealso;
  <simplelist>
   <member><function>sapi_windows_cp_set</function></member>
  </simplelist>
 </refsect1><!-- }}} -->

</refentry>

