<refentry xml:id="function.apd-dump-persistent-resources" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>apd_dump_persistent_resources</refname>
  <refpurpose>Return all persistent resources as an array</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>apd_dump_persistent_resources</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Return all persistent resources as an array.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   An array containing the current persistent resources.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>apd_dump_persistent_resources</function> example</title>
    <programlisting role="php">
<![CDATA[
<?php
print_r(apd_dump_persistent_resources());
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>apd_dump_regular_resources</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

