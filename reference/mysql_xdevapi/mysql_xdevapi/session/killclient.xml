<refentry xml:id="mysql-xdevapi-session.killclient" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Session::killClient</refname>
  <refpurpose>Kill the client</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>object</type><methodname>mysql_xdevapi\Session::killClient</methodname>
   <methodparam><type>integer</type><parameter>client_id</parameter></methodparam>
  </methodsynopsis>
  <para>
   Kill the selected client and terminate the collection
  </para>

  &warn.undocumented.func;

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>client_id</parameter></term>
    <listitem>
     <para>
      A connection's client ID.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   
  </para>
 </refsect1>

<refsect1 role="examples">
  &reftitle.examples;
  <example>
   <title><function>mysql_xdevapi\Session::killClient</function> example</title>
   <programlisting role="php">
<![CDATA[
<?php
$session = mysql_xdevapi\getSession("mysqlx://user:password@localhost");

$clientid = $session->getClientId();

// ...

$session->killClient($clientid);
]]>
   </programlisting>
  </example>
 </refsect1>

</refentry>

